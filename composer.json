{
    "name": "illuminatech/multipart-middleware",
    "description": "'multipart/form-data' parser middleware for Laravel",
    "keywords": ["laravel", "middleware", "multipart", "request", "parser"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/illuminatech/multipart-middleware/issues",
        "wiki": "https://github.com/illuminatech/multipart-middleware/wiki",
        "source": "https://github.com/illuminatech/multipart-middleware"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "illuminate/http": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Illuminatech\\MultipartMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminatech\\MultipartMiddleware\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}